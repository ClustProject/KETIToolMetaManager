{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"domain": {
			"type": "string"
		},
		"sub_domain": {
			"type": "string"
		},
		"table_name": {
			"type": "string"
		},
		"start_time": {
			"type": "string"
		},
		"end_time": {
			"type": "string"
		},
		"tags":{
			"type": "array",
			"items": {
				"type":"string"
			}
		},
		"fields": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"fieldKey": {
						"type": "string"
					},
					"fieldType": {
						"type": "string"
					}
				},
				"required": [
					"fieldKey", "fieldType"
				]
			}
		},
		"number_of_columns": {
			"type": "integer"
		},
		"location": {
			"type": "object",
			"properties": {
				"lat": {
					"type": "number"
				},
				"lng": {
					"type": "number"
				},
				"syntax": {
					"type": "string"
				}
			}
		},
		"description": {
			"type": "string"
		},
		"tag": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"frequency": {
			"type": "string"
		},
		"column_information": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"column_name": {
						"type": "string"
					},
					"analysisResult": {
						"type": "object",
						"properties": {
							"ListBasedResult": {
								"type": "array",
								"items": {
									"type": "object",
									"properties": {
										"analysisName": {
											"type": "string"
										},
										"characteristics": {
											"enum": [
												"mean", "count"
											]
										},
										"result": {
											"type": "array",
											"items": {
												"type": "object",
												"properties": {
													"labelNameList": {
														"type": "array",
														"items": {
															"type": "string"
														}
													},
													"labelValueList": {
														"type": "array",
														"items": {
															"type": "number"
														}
													}
												},
												"required": [
													"labelNameList", "labelValueList"
												]
											}
										}
									},
									"required": [
										"analysisName", "characteristics", "result"
									]
								}
							}
						}
					}
				},
				"required": [
					"column_name"
				]
			}
		}
	},
	"required": [
		"domain", "sub_domain", "table_name"
	]
}